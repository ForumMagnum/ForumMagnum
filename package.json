{
  "name": "lesswrong",
  "version": "2.1.0",
  "scripts": {
    "run-next": "next dev --turbopack",
    "postinstall": "scripts/postinstall.sh",
    "checktypes": "scripts/lint_and_checktypes.sh",
    "lint": "scripts/lint_and_checktypes.sh",
    "tsc": "tsc --noEmit --skipLibCheck",
    "eslint": "eslint --max-warnings 0 --cache --ext .jsx,js,tsx,ts packages",
    "build": "ts-node --transpile-only ./build.ts",
    "migrate": "ts-node -r tsconfig-paths/register --project tsconfig-repl.json ./migrate.ts",
    "gfrepl": "NODE_OPTIONS='--inspect --no-deprecation' SETTINGS_FILE=settings-local-dev-devdb-ea.json ts-node --swc -r tsconfig-paths/register --project tsconfig-repl.json scripts/repl.ts",
    "repl": "NODE_OPTIONS='--inspect --no-deprecation' ts-node --swc -r tsconfig-paths/register --project tsconfig-repl.json scripts/repl.ts",
    "upload-ckeditor-bundle": "ts-node --swc -r tsconfig-paths/register --project tsconfig-repl.json ./deployCkEditorBundle.ts",
    "branchdb": "ts-node -r tsconfig-paths/register scripts/branchDb.ts",
    "start": "yarn start-local-db",
    "start-local-db": "ts-node --transpile-only ./build.ts -run --watch --lint --settings settings-dev.json --db ../LessWrong-Credentials/connectionConfigs/local.json",
    "start-dev-db": "ts-node --transpile-only ./build.ts --watch --lint --db ../LessWrong-Credentials/connectionConfigs/dev.json --settings ../LessWrong-Credentials/settings-local-dev-devdb.json",
    "start-dev-db-xpost": "PORT=4000 ts-node --transpile-only ./build.ts --watch --lint --db ../LessWrong-Credentials/connectionConfigs/dev.json --settings ./settings-test-crosspost.json",
    "start-prod-db": "ts-node --transpile-only ./build.ts --no-clear -run --watch --lint --db ../LessWrong-Credentials/connectionConfigs/prod.json --settings ../LessWrong-Credentials/settings-local-dev-proddb.json",
    "lw-start-local-db": "ts-node --transpile-only ./build.ts --no-clear -run --watch --lint --db ../LessWrong-Credentials/connectionConfigs/local.json --settings settings.json",
    "shell": "ts-node --transpile-only ./build.ts --no-clear -run --settings settings.json --shell",
    "shell-dev-db": "ts-node --transpile-only ./build.ts --no-clear -run --db ../LessWrong-Credentials/connectionConfigs/dev.json --settings ../LessWrong-Credentials/settings-local-dev-devdb.json --shell",
    "shell-prod-db": "ts-node --transpile-only ./build.ts --no-clear -run --db ../LessWrong-Credentials/connectionConfigs/prod.json --settings ../LessWrong-Credentials/settings-local-dev-proddb.json --shell",
    "start-af": "ts-node --transpile-only ./build.ts --no-clear -run --watch --lint --settings settings-af.json",
    "start-af-dev-db": "ts-node --transpile-only ./build.ts --no-clear -run --watch --lint --db ../LessWrong-Credentials/connectionConfigs/dev.json --settings ../LessWrong-Credentials/settings-local-dev-af-devdb.json",
    "start-af-prod-db": "ts-node --transpile-only ./build.ts --no-clear -run --watch --lint --db ../LessWrong-Credentials/connectionConfigs/prod.json --settings ../LessWrong-Credentials/settings-local-dev-af-proddb.json",
    "unit": "jest --config jest.unit.config.ts --forceExit",
    "unit-ci": "jest --config jest.unit.config.ts --forceExit --ci --coverage --coverageProvider=v8",
    "unit-watch": "jest --config jest.unit.config.ts --watchAll",
    "unit-coverage": "jest --config jest.unit.config.ts --forceExit --coverage --coverageProvider=v8",
    "exec-accepted-schema": "psql $PG_URL -f ./schema/accepted_schema.sql",
    "create-integration-db": "PG_URL=$(cat ../ForumCredentials/testing-pg-conn.txt) ./scripts/createIntegrationPgDb.sh",
    "create-integration-db-ci": "yarn exec-accepted-schema && ./scripts/createIntegrationPgDb.sh",
    "integration": "PG_URL=$(cat ../ForumCredentials/testing-pg-conn.txt) PG_MAX_CONNECTIONS=5 jest --config jest.integration.config.ts --forceExit",
    "integration-ci": "PG_MAX_CONNECTIONS=5 jest --config jest.integration.config.ts --forceExit --ci --coverage --coverageProvider=v8",
    "integration-watch": "jest --config jest.integration.config.ts --watchAll",
    "integration-coverage": "jest --config jest.integration.config.ts --forceExit --coverage --coverageProvider=v8",
    "combine-coverage": "./scripts/combineCoverage.sh",
    "encrypt-credentials": "tar czf credentials.tar.gz --exclude '.git/*' -C ../ ./LessWrong-Credentials; travis encrypt-file credentials.tar.gz --add; rm credentials.tar.gz",
    "generate": "scripts/generateTypes.sh",
    "rebuild-ckeditor": "cd ./ckEditor && yarn install && yarn run build",
    "production": "scripts/runProduction.sh",
    "deploy": "scripts/deploy.sh",
    "ea-command-dev": "ts-node --transpile-only ./build.ts -run --settings ../ForumCredentials/settings-dev.json --postgresUrlFile ../ForumCredentials/dev-pg-conn.txt --command",
    "ea-command-staging-db": "ts-node --transpile-only ./build.ts -run --settings ../ForumCredentials/settings-dev.json --postgresUrlFile ../ForumCredentials/dev-pg-conn.txt --command",
    "ea-command-prod-db": "ts-node --transpile-only ./build.ts -run --settings ../ForumCredentials/settings-dev.json --postgresUrlFile ../ForumCredentials/dev-pg-conn.txt --command",
    "ea-command-testing": "ts-node --transpile-only ./build.ts -run --settings ../ForumCredentials/settings-dev.json --postgresUrlFile ../ForumCredentials/testing-pg-conn.txt --command",
    "ea-command-ci": "ts-node --transpile-only ./build.ts -run --settings ./settings-test.json --postgresUrl $PG_URL --command",
    "ea-start": "ts-node --transpile-only ./build.ts -run -watch --lint --settings ../ForumCredentials/settings-dev.json --postgresUrlFile ../ForumCredentials/dev-pg-conn.txt",
    "ea-start-xpost-dev": "PORT=4000 ts-node --transpile-only ./build.ts -run -watch --lint --settings ../ForumCredentials/settings-xpost.json --postgresUrlFile ../ForumCredentials/xpost-pg-conn.txt",
    "playwright-db": "docker run --rm --name forum-magnum-test-postgres -e POSTGRES_PASSWORD=password -p 5433:5432 -v $PWD/schema/accepted_schema.sql:/docker-entrypoint-initdb.d/00_accepted_schema.sql -v $PWD/playwright/fixtures.sql:/docker-entrypoint-initdb.d/01_fixtures.sql ankane/pgvector",
    "playwright-db-crosspost": "docker run --rm --name forum-magnum-test-postgres-crosspost -e POSTGRES_PASSWORD=password -p 5434:5432 -v $PWD/schema/accepted_schema.sql:/docker-entrypoint-initdb.d/00_accepted_schema.sql -v $PWD/playwright/fixtures.sql:/docker-entrypoint-initdb.d/01_fixtures.sql ankane/pgvector",
    "playwright-test": "playwright test",
    "playwright-test-crosspost": "CROSSPOST_TEST=true playwright test",
    "start-playwright": "PORT=3456 ts-node --transpile-only ./build.ts -run --watch --e2e --settings ./settings-test.json --postgresUrl postgres://postgres:password@localhost:5433/postgres",
    "start-playwright-crosspost": "PORT=3467 ts-node --transpile-only ./build.ts -run --watch --e2e --settings ./settings-test-crosspost.json --postgresUrl postgres://postgres:password@localhost:5434/postgres",
    "ea-start-staging-db": "ts-node --transpile-only ./build.ts -run --watch --lint --settings ../ForumCredentials/settings-local-dev-staging-db.json --postgresUrlFile ../ForumCredentials/staging-pg-conn.txt",
    "ea-start-prod-db": "ts-node --transpile-only ./build.ts -run --watch --lint --settings ../ForumCredentials/settings-local-dev-prod-db.json --postgresUrlFile ../ForumCredentials/prod-pg-conn.txt",
    "ea-start-testing-db": "ts-node --transpile-only ./build.ts -run --watch --e2e --settings ./settings-test.json --postgresUrl ${PG_URL:=$(cat ../ForumCredentials/testing-pg-conn.txt)}",
    "start-ci": "ts-node --transpile-only ./build.ts -run --settings ./settings-test.json --postgresUrl postgres://postgres:password@localhost:5433/postgres",
    "ea-pg": "psql $(< ../ForumCredentials/dev-pg-conn.txt)",
    "ea-pg-xpost-dev": "psql $(< ../ForumCredentials/xpost-pg-conn.txt)",
    "ea-pg-staging": "psql $(< ../ForumCredentials/staging-pg-conn.txt)",
    "ea-pg-testing": "psql $(< ../ForumCredentials/testing-pg-conn.txt)",
    "ea-pg-prod": "psql $(< ../ForumCredentials/prod-pg-conn.txt)",
    "ea-pg-dev-admin": "psql $(< ../ForumCredentials/dev-staging-admin-pg-conn.txt)",
    "ea-pg-prod-admin": "psql $(< ../ForumCredentials/prod-admin-pg-conn.txt)",
    "ea-load-dev-db": "ts-node -r tsconfig-paths/register ./scripts/loadDbSettings.ts dev",
    "ea-load-xpost-dev-db": "ts-node -r tsconfig-paths/register ./scripts/loadDbSettings.ts xpost",
    "ea-load-staging-db": "ts-node -r tsconfig-paths/register ./scripts/loadDbSettings.ts staging",
    "ea-load-prod-db": "node -r tsconfig-paths/register ../ForumCredentials/assertSafeToUpdateDbSettings.js && ts-node -r tsconfig-paths/register ./scripts/loadDbSettings.ts prod",
    "ea-load-testing-db": "ts-node -r tsconfig-paths/register ./scripts/loadDbSettings.ts testing",
    "ea-replicate-dev-db": "./scripts/eaReplicateDevDb.sh",
    "generate-client-stats": "esbuild-visualizer --metadata ./client_meta.json --filename client_stats.html",
    "brigader-check": "ts-node -r tsconfig-paths/register scripts/brigaderCheck.ts",
    "create-collection": "scripts/createCollection.sh",
    "graphql-codegen": "graphql-codegen --config codegen.ts",
