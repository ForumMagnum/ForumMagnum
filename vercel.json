{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "git": {
    "deploymentEnabled": {
      "master": false
    }
  },
  "crons": [{
    "path": "/api/cron/every-minute",
    "schedule": "* * * * *"
  }, {
    "path": "/api/cron/every-five-minutes",
    "schedule": "*/5 * * * *"
  }, {
    "path": "/api/cron/every-ten-minutes",
    "schedule": "*/10 * * * *"
  }, {
    "path": "/api/cron/update-analytics-collections",
    "schedule": "*/20 * * * *"
  }, {
    "path": "/api/cron/update-promoted-spotlight-item",
    "schedule": "*/30 * * * *"
  }, {
    "path": "/api/cron/run-twitter-bot",
    "schedule": "*/31 * * * *"
  }, {
    "path": "/api/cron/every-hour",
    "schedule": "0 * * * *"
  }, {
    "path": "/api/cron/update-user-activities",
    "schedule": "0 */3 * * *"
  }, {
    "path": "/api/cron/every-midnight",
    "schedule": "0 0 * * *"
  }, {
    "path": "/api/cron/update-missing-post-embeddings",
    "schedule": "0 1 * * *"
  }, {
    "path": "/api/cron/update-review-vote-totals",
    "schedule": "0 2 * * *"
  }, {
    "path": "/api/cron/curation-status-to-slack",
    "schedule": "0 19 * * *"
  }]
}
